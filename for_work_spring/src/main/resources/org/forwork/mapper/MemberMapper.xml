<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">



<mapper namespace="org.forwork.mapper.MemberMapper">
	<cache />
	
	<insert id="insertMember" parameterType="org.forwork.domain.Member">
		insert into Member(
			member_id, name, email, pw
		)
		values(
			MEMBER_SEQ.NEXTVAL, #{name}, #{email}, #{pw}
		)
	</insert>
	<insert id="insertAuth" parameterType="org.forwork.domain.Member">
		insert into member_auth(
			member_id, auth
		)
		values(
			email,auth
		)
	</insert>
	
	
	<!-- <select id="getMember" parameterType="String" resultType="Member">
		select * from member where member_id = #{member_id}
	</select> -->
	
</mapper>















